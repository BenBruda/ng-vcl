{
  "name": "ng-vcl",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "demo": "npm run dev",
    "dev": "npm-run-all dev:vcl --parallel dev:serve dev:styles",
    "dev:serve": "webpack-dev-server --env.ENV development",
    "dev:vcl": "vcl-preprocessor demo/styles/index.styl | postcss --use autoprefixer -o demo/app.css",
    "dev:styles": "fsmonitor -d 'demo/styles' npm run dev:vcl",
    "build:copy": "node scripts/copy-html.js",
    "build:cjs": "tsc --declaration -p tsconfig.cjs.json",
    "build:esm": "tsc --declaration -p tsconfig.esm.json",
    "build:esm-rollup": "rollup -c rollup.config.js",
    "build": "npm-run-all build:copy build:cjs build:esm-rollup",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ng-vcl/ng-vcl.git"
  },
  "keywords": [
    "web-component",
    "angular",
    "VCL"
  ],
  "author": "The ng-vcl authors",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "jsnext:main": "dist/ng-vcl.esm.js",
  "module": "dist/ng-vcl.esm.js",
  "jspm": {
    "main": "dist/ng-vcl.esm.js",
    "format": "esm"
  },
  "peerDependencies": {
    "@angular/core": "^2.0.0-rc"
  },
  "devDependencies": {
    "@angular/common": "^2.0.0-rc.5",
    "@angular/compiler": "^2.0.0-rc.5",
    "@angular/compiler-cli": "^0.5.0",
    "@angular/core": "^2.0.0-rc.5",
    "@angular/http": "^2.0.0-rc.5",
    "@angular/forms": "0.3.0",
    "@angular/platform-browser": "^2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.5",
    "@angular/router": "^3.0.0-rc.1",
    "@types/core-js": "^0.9.28",
    "@types/tether": "^1.1.26",
    "@types/requirejs": "^2.1.26",
    "angular2-template-loader": "^0.4.0",
    "autoprefixer": "^6.3.6",
    "awesome-typescript-loader": "^2.1.1",
    "browser-sync": "^2.14.0",
    "connect-history-api-fallback": "^1.2.0",
    "core-js": "^2.4.1",
    "cpr": "^1.1.2",
    "express": "^4.14.0",
    "fsmonitor": "^0.2.4",
    "ignore-loader": "^0.1.1",
    "npm-run-all": "^2.3.0",
    "postcss-cli": "^2.5.2",
    "raw-loader": "^0.5.1",
    "rollup": "^0.34.10",
    "rollup-plugin-angular": "^0.2.3",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rxjs": "5.0.0-beta.11",
    "typescript": "^2.0.0",
    "vcl-core-modules": "^0.1.0",
    "vcl-default-theme": "^0.1.0",
    "vcl-default-theme-terms": "^0.1.0",
    "vcl-font-awesome": "^0.1.0",
    "vcl-preprocessor": "^0.1.0",
    "webpack": "^2.1.0-beta.20",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^2.1.0-beta.0",
    "zone.js": "0.6.12"
  },
  "dependencies": {
    "hammerjs": "^2.0.8",
    "tether": "^1.3.4"
  }
}
