{
  "name": "ng-vcl",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "demo": "npm run dev",
    "dev": "npm-run-all dev:vcl --parallel dev:serve dev:styles",
    "dev:serve": "webpack-dev-server --env.ENV development --host 0.0.0.0 --content-base docs/",
    "dev:vcl": "vcl-preprocessor demo/styles/index.styl | postcss --use autoprefixer -o demo/app.css",
    "dev:styles": "fsmonitor -d 'demo/styles' npm run dev:vcl",
    "docs:build": "webpack --env.ENV production",
    "docs:push": "git subtree push --prefix docs origin gh-pages",
    "build:lint": "tslint src/**/*.ts",
    "build:ngc": "ngc -p tsconfig.build.json",
    "build:html": "node copy.js",
    "build:bundles": "rollup -c rollup.config.js",
    "build": "npm-run-all build:lint build:bundles build:ngc build:html",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "npm run build:lint && tslint demo/**/*.ts",
    "docker": "docker run -it -v $(pwd):/app -p 0.0.0.0:3000:3000 node:6.9.1 /bin/bash /app/entrypoint.bash"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ng-vcl/ng-vcl.git"
  },
  "keywords": [
    "web-component",
    "angular",
    "VCL"
  ],
  "author": "The ng-vcl authors",
  "license": "MIT",
  "main": "dist/ng-vcl.cjs.js",
  "typings": "dist/index.d.ts",
  "jsnext:main": "dist/ng-vcl.esm.js",
  "module": "dist/ng-vcl.esm.js",
  "jspm": {
    "main": "dist/ng-vcl.esm.js",
    "format": "esm"
  },
  "peerDependencies": {
    "@angular/core": "^2.0.0-rc"
  },
  "devDependencies": {
    "@angular/common": "^2.1.2",
    "@angular/compiler": "^2.1.2",
    "@angular/compiler-cli": "^2.1.2",
    "@angular/core": "^2.1.2",
    "@angular/forms": "2.1.2",
    "@angular/http": "^2.1.2",
    "@angular/platform-browser": "^2.1.2",
    "@angular/platform-browser-dynamic": "^2.1.2",
    "@angular/router": "^3.1.2",
    "@types/core-js": "^0.9.34",
    "@types/marked": "0.0.28",
    "@types/requirejs": "^2.1.28",
    "@types/tether": "^1.1.27",
    "angular2-template-loader": "^0.6.0",
    "autoprefixer": "^6.5.1",
    "awesome-typescript-loader": "^2.2.4",
    "browser-sync": "^2.17.5",
    "connect-history-api-fallback": "1.3.0",
    "core-js": "^2.4.1",
    "cpr": "^2.0.0",
    "css-loader": "^0.25.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "fsmonitor": "^0.2.4",
    "ignore-loader": "^0.1.1",
    "json-loader": "^0.5.4",
    "npm-run-all": "^3.1.1",
    "postcss-cli": "^2.6.0",
    "raw-loader": "^0.5.1",
    "rollup": "^0.36.3",
    "rollup-plugin-angular": "^0.4.3",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rxjs": "5.0.0-rc.1",
    "style-loader": "^0.13.1",
    "tslint": "^3.15.1",
    "typescript": "^2.0.6",
    "vcl-core-modules": "^0.1.1",
    "vcl-default-theme": "^0.1.2",
    "vcl-default-theme-terms": "^0.1.0",
    "vcl-font-awesome": "^0.1.0",
    "vcl-preprocessor": "^0.1.1",
    "webpack": "2.1.0-beta.20",
    "webpack-dev-server": "2.1.0-beta.3",
    "zone.js": "0.6.26"
  },
  "dependencies": {
    "fuse.js": "^2.5.0",
    "hammerjs": "^2.0.8",
    "jsoneditor": "5.5.10",
    "marked": "^0.3.6",
    "tether": "^1.3.7"
  }
}
