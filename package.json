{
  "name": "@ng-vcl/ng-vcl",
  "version": "0.3.3-6",
  "description": "A comprehensive library of components for Angular 4 with VCL based styling",
  "scripts": {
    "lint": "tslint --type-check -p ./tsconfig.json",
    "lint:src": "tslint --type-check -p ./src/tsconfig.json",
    "dev": "webpack-dev-server --config webpack.config.js --env.ENV=development",
    "dev:aot": "webpack-dev-server --config webpack.config.js --env.ENV=development --env.AOT=true",
    "docs:clean": "rimraf docs",
    "docs:build": "webpack --config webpack.config.js --env.ENV=production --env.AOT=true",
    "docs:deploy": "git-directory-deploy --directory docs --branch gh-pages",
    "docs": "npm-run-all lint docs:clean docs:build",
    "build:clean": "rimraf dist",
    "build:gulp": "gulp build",
    "build": "npm-run-all lint:src build:clean build:gulp",
    "publish": "gulp publish",
    "publish:next": "gulp publish:next",
    "publish:git:ng-vcl": "git-directory-deploy --directory dist/@ng-vcl/ng-vcl --branch dist_v0.3",
    "publish:git:ng-vcl-c": "git-directory-deploy --directory dist/@ng-vcl/ng-vcl-compatibility --branch dist_v0.3-compatibility",
    "clean": "npm-run-all build:clean docs:clean",
    "docker": "docker run -it -v $(pwd):/app -p 0.0.0.0:3000:3000 node:6.9.1 /bin/bash /app/entrypoint.bash",
    "postinstall": "ts-node scripts/postinstall.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ng-vcl/ng-vcl.git"
  },
  "keywords": [
    "web-component",
    "angular",
    "VCL"
  ],
  "author": "The ng-vcl authors",
  "license": "MIT",
  "devDependencies": {
    "@angular/animations": "^4.2.3",
    "@angular/common": "^4.2.3",
    "@angular/compiler": "^4.2.3",
    "@angular/compiler-cli": "^4.2.3",
    "@angular/core": "^4.2.3",
    "@angular/forms": "^4.2.3",
    "@angular/http": "^4.2.3",
    "@angular/platform-browser": "^4.2.3",
    "@angular/platform-browser-dynamic": "^4.2.3",
    "@angular/router": "^4.2.3",
    "@ngtools/webpack": "^1.4.0",
    "@types/gulp": "^4.0.2",
    "@types/node": "^7.0.31",
    "@types/tether": "^1.4.2",
    "angular-router-loader": "^0.6.0",
    "angular2-template-loader": "~0.6.2",
    "autoprefixer": "^7.1.1",
    "awesome-typescript-loader": "^3.1.3",
    "chance": "^1.0.10",
    "copy-webpack-plugin": "~4.0.1",
    "core-js": "~2.4.1",
    "cpr": "^2.1.0",
    "css-loader": "^0.28.1",
    "es6-promisify": "^5.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.2",
    "fsmonitor": "~0.2.4",
    "fuse.js": "~2.6.2",
    "git-directory-deploy": "~1.5.1",
    "gulp": "^3.9.1",
    "gulp-better-rollup": "^1.1.1",
    "gulp-json-modify": "^1.0.2",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "hammerjs": "^2.0.8",
    "highlight-loader": "~0.7.2",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "~2.28.0",
    "ignore-loader": "~0.1.2",
    "json-loader": "~0.5.4",
    "jsoneditor": "^5.6.0",
    "jsonschema": "^1.1.1",
    "markdown-loader": "^2.0.0",
    "marked": "~0.3.6",
    "merge-stream": "^1.0.1",
    "npm-run-all": "^4.0.2",
    "plotly.js": "^1.28.1",
    "postcss-loader": "^2.0.6",
    "pre-commit": "~1.2.2",
    "raw-loader": "~0.5.1",
    "resolve-bin": "^0.4.0",
    "rimraf": "^2.6.1",
    "rollup-plugin-replace": "^1.1.1",
    "run-sequence": "^1.2.2",
    "rxjs": "^5.4.1",
    "style-loader": "^0.18.2",
    "ts-node": "^3.0.4",
    "tslint": "^5.4.2",
    "typescript": "^2.3.2",
    "url-loader": "^0.5.9",
    "vcl-core-modules": "^0.1.4",
    "vcl-default-theme": "~0.1.3",
    "vcl-default-theme-terms": "~0.1.0",
    "vcl-font-awesome": "~0.1.0",
    "vcl-loader": "github:vcl/loader",
    "vcl-preprocessor": "~0.1.1",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0",
    "zone.js": "^0.8.5"
  },
  "pre-commit": [
    "lint"
  ]
}
