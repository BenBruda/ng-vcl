{
  "name": "@ng-vcl/ng-vcl",
  "version": "0.3.17",
  "description": "A comprehensive library of components for Angular 4 with VCL based styling",
  "scripts": {
    "lint": "tslint --type-check -p ./tsconfig.json",
    "lint:src": "tslint --type-check -p ./src/tsconfig.json",
    "dev": "webpack-dev-server --config webpack.config.js --env.ENV=development",
    "dev:aot": "webpack-dev-server --config webpack.config.js --env.ENV=development --env.AOT=true",
    "docs:clean": "rimraf docs",
    "docs:build": "webpack --config webpack.config.js --env.ENV=production --env.AOT=true",
    "docs:deploy": "git-directory-deploy --directory docs --branch gh-pages",
    "docs": "npm-run-all lint docs:clean docs:build",
    "build:clean": "rimraf dist",
    "build:gulp": "gulp build",
    "build": "npm-run-all lint:src build:clean build:gulp",
    "publish": "gulp publish",
    "publish:git:ng-vcl": "git-directory-deploy --directory dist/@ng-vcl/ng-vcl --branch dist_v0.3",
    "publish:git:ng-vcl-c": "git-directory-deploy --directory dist/@ng-vcl/ng-vcl-compatibility --branch dist_v0.3-compatibility",
    "clean": "npm-run-all build:clean docs:clean",
    "docker": "docker run -it -v $(pwd):/app -p 0.0.0.0:3000:3000 node:6.9.1 /bin/bash /app/entrypoint.bash",
    "postinstall": "ts-node --compilerOptions '{\"target\":\"es5\",\"module\":\"CommonJS\"}'  scripts/postinstall.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ng-vcl/ng-vcl.git"
  },
  "keywords": [
    "web-component",
    "angular",
    "VCL"
  ],
  "author": "The ng-vcl authors",
  "license": "MIT",
  "devDependencies": {
    "@angular/animations": "^5.0.0-rc.9",
    "@angular/common": "^5.0.0-rc.9",
    "@angular/compiler": "^5.0.0-rc.9",
    "@angular/compiler-cli": "^5.0.0-rc.9",
    "@angular/core": "^5.0.0-rc.9",
    "@angular/forms": "^5.0.0-rc.9",
    "@angular/http": "^5.0.0-rc.9",
    "@angular/platform-browser": "^5.0.0-rc.9",
    "@angular/platform-browser-dynamic": "^5.0.0-rc.9",
    "@angular/router": "^5.0.0-rc.9",
    "@ngtools/webpack": "^1.7.4",
    "@types/gulp": "^4.0.5",
    "@types/node": "^8.0.47",
    "@types/tether": "^1.4.3",
    "@vcl/breakpoints": "^0.2.0",
    "@vcl/core-modules": "^0.2.2",
    "@vcl/default-theme": "^0.2.1",
    "@vcl/default-theme-terms": "^0.2.1",
    "@vcl/font-awesome": "^0.2.0",
    "@vcl/preprocessor": "^0.2.2",
    "angular-router-loader": "^0.6.0",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^7.1.6",
    "awesome-typescript-loader": "^3.2.3",
    "chance": "^1.0.11",
    "copy-webpack-plugin": "^4.2.0",
