    "@angular/platform-browser-dynamic": "^5.1.3",
    "@angular/router": "^5.1.3",
    "@ngtools/webpack": "^1.9.3",
    "@types/gulp": "^4.0.5",
    "@types/node": "^8.0.54",
    "@types/tether": "^1.4.3",
