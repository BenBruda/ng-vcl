{
  "name": "@ng-vcl/ng-vcl",
  "version": "0.1.3",
  "description": "A comprehensive library of components for Angular 2 with VCL based styling",
  "scripts": {
    "lint": "npm-run-all lint:src lint:demo",
    "lint:src": "tslint './src/**/*.ts'",
    "lint:demo": "tslint './src/**/*.ts'",
    "dev": "npm-run-all dev:vcl --parallel dev:serve dev:styles",
    "dev:serve": "webpack-dev-server --config webpack.config.js --env.ENV development --env.HMR true",
    "dev:vcl": "vcl-preprocessor demo/styles/index.styl | postcss --use autoprefixer -o demo/app.css",
    "dev:styles": "fsmonitor -d 'demo/styles' npm run dev:vcl",
    "docs:clean": "rimraf docs",
    "docs:build": "webpack --config webpack.config.js --env.ENV production",
    "docs:deploy": "git-directory-deploy --directory docs --branch gh-pages",
    "docs": "npm-run-all lint docs:clean docs:build",
    "build:clean": "rimraf dist",
    "build:gulp": "gulp build",
    "build": "npm-run-all lint:src build:clean build:gulp",
    "publish": "gulp publish",
    "clean": "npm-run-all build:clean docs:clean",
    "docker": "docker run -it -v $(pwd):/app -p 0.0.0.0:3000:3000 node:6.9.1 /bin/bash /app/entrypoint.bash",
    "postinstall": "ts-node scripts/postinstall.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ng-vcl/ng-vcl.git"
  },
  "keywords": [
    "web-component",
    "angular",
    "VCL"
  ],
  "files": [
    "dist"
  ],
  "author": "The ng-vcl authors",
  "license": "MIT",
  "main": "dist/ng-vcl.js",
  "typings": "dist/index.d.ts",
  "peerDependencies": {
    "@angular/common": "^2.3.0",
    "@angular/core": "^2.3.0",
    "@angular/forms": "^2.3.0",
    "@angular/http": "^2.3.0",
    "@angular/platform-browser": "^2.3.0",
    "@angular/router": "^3.3.0",
    "rxjs": "^5.0.3",
    "hammerjs": "^2.0.8"
  },
  "devDependencies": {
    "@angular/common": "^2.4.5",
    "@angular/compiler": "^2.4.5",
    "@angular/compiler-cli": "^2.4.5",
    "@angular/core": "^2.4.5",
    "@angular/forms": "^2.4.5",
    "@angular/http": "^2.4.5",
    "@angular/platform-browser": "^2.4.5",
    "@angular/platform-browser-dynamic": "^2.4.5",
    "@angular/router": "^3.4.5",
    "@types/gulp": "^3.8.32",
    "@types/marked": "~0.0.28",
    "@types/node": "^7.0.5",
    "@types/tether": "~1.4.0",
    "angular2-template-loader": "~0.6.0",
    "autoprefixer": "~6.7.2",
    "awesome-typescript-loader": "3.0.4-rc.2",
    "copy-webpack-plugin": "~4.0.1",
    "core-js": "~2.4.1",
    "cpr": "~2.0.2",
    "css-loader": "~0.26.1",
    "es6-promisify": "^5.0.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "fsmonitor": "~0.2.4",
    "fuse.js": "~2.6.1",
    "git-directory-deploy": "~1.5.1",
    "gulp": "^3.9.1",
    "gulp-better-rollup": "^1.0.2",
    "gulp-json-modify": "^1.0.2",
    "hammerjs": "^2.0.8",
    "highlight-loader": "~0.7.2",
    "html-loader": "~0.4.4",
    "html-webpack-plugin": "~2.28.0",
    "ignore-loader": "~0.1.2",
    "json-loader": "~0.5.4",
    "jsoneditor": "^5.5.11",
    "jsonschema": "^1.1.1",
    "markdown-loader": "~0.1.7",
    "marked": "~0.3.6",
    "npm-run-all": "~4.0.1",
    "plotly.js": "^1.23.1",
    "postcss-cli": "~2.6.0",
    "pre-commit": "~1.2.2",
    "raw-loader": "~0.5.1",
    "resolve-bin": "^0.4.0",
    "rimraf": "~2.5.4",
    "run-sequence": "^1.2.2",
    "rxjs": "^5.1.0",
    "style-loader": "~0.13.1",
    "ts-node": "^2.1.0",
    "tslint": "^4.4.2",
    "typescript": "^2.1.5",
    "vcl-core-modules": "~0.1.3",
    "vcl-default-theme": "~0.1.3",
    "vcl-default-theme-terms": "~0.1.0",
    "vcl-font-awesome": "~0.1.0",
    "vcl-preprocessor": "~0.1.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "~2.2.0",
    "zone.js": "^0.7.6"
  },
  "pre-commit": [
    "lint"
  ]
}
