{
  "name": "@ng-vcl/ng-vcl",
  "version": "0.3.1-1",
  "description": "A comprehensive library of components for Angular 2 with VCL based styling",
  "scripts": {
    "lint": "npm-run-all lint:src lint:demo",
    "lint:src": "tslint './src/**/*.ts'",
    "lint:demo": "tslint './demo/**/*.ts'",
    "dev:aot": "npm-run-all dev:vcl --parallel \"dev:serve -- --env.AOT=true --env.DEMO={*}\" dev:styles --",
    "dev": "npm-run-all dev:vcl --parallel \"dev:serve -- --env.DEMO={*}\" dev:styles --",
    "dev:serve": "webpack-dev-server --config webpack.config.js --env.ENV development --env.HMR true",
    "dev:vcl": "vcl-preprocessor demo/styles/index.styl | postcss --use autoprefixer -o demo/app.css",
    "dev:styles": "fsmonitor -d 'demo/styles' npm run dev:vcl",
    "docs:clean": "rimraf docs",
    "docs:build": "webpack --config webpack.config.js --env.ENV production",
    "docs:deploy": "git-directory-deploy --directory docs --branch gh-pages",
    "docs": "npm-run-all lint docs:clean docs:build",
    "build:clean": "rimraf dist",
    "build:gulp": "gulp build",
    "build": "npm-run-all lint:src build:clean build:gulp",
    "publish": "gulp publish",
    "publish:next": "gulp publish:next",
    "clean": "npm-run-all build:clean docs:clean",
    "docker": "docker run -it -v $(pwd):/app -p 0.0.0.0:3000:3000 node:6.9.1 /bin/bash /app/entrypoint.bash",
    "postinstall": "ts-node scripts/postinstall.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ng-vcl/ng-vcl.git"
  },
  "keywords": [
    "web-component",
    "angular",
    "VCL"
  ],
  "author": "The ng-vcl authors",
  "license": "MIT",
  "devDependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@ngtools/webpack": "^1.3.0",
    "@types/gulp": "^4.0.2",
    "@types/marked": "~0.0.28",
    "@types/node": "^7.0.5",
    "@types/tether": "~1.4.0",
    "angular-router-loader": "^0.6.0",
    "angular2-template-loader": "~0.6.2",
    "autoprefixer": "~6.7.6",
    "awesome-typescript-loader": "^3.1.2",
    "chance": "^1.0.6",
    "copy-webpack-plugin": "~4.0.1",
    "core-js": "~2.4.1",
    "cpr": "~2.0.2",
    "css-loader": "^0.27.3",
    "es6-promisify": "^5.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "fsmonitor": "~0.2.4",
    "fuse.js": "~2.6.2",
    "git-directory-deploy": "~1.5.1",
    "gulp": "^3.9.1",
    "gulp-better-rollup": "^1.1.1",
    "gulp-json-modify": "^1.0.2",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "hammerjs": "^2.0.8",
    "highlight-loader": "~0.7.2",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "~2.28.0",
    "ignore-loader": "~0.1.2",
    "json-loader": "~0.5.4",
    "jsoneditor": "^5.5.11",
    "jsonschema": "^1.1.1",
    "markdown-loader": "^2.0.0",
    "marked": "~0.3.6",
    "merge-stream": "^1.0.1",
    "npm-run-all": "^4.0.2",
    "plotly.js": "^1.25.0",
    "postcss-cli": "~3.0.0",
    "pre-commit": "~1.2.2",
    "raw-loader": "~0.5.1",
    "resolve-bin": "^0.4.0",
    "rimraf": "^2.6.1",
    "rollup-plugin-replace": "^1.1.1",
    "run-sequence": "^1.2.2",
    "rxjs": "^5.2.0",
    "style-loader": "^0.15.0",
    "ts-node": "^3.0.2",
    "tslint": "^4.5.1",
    "typescript": "~2.1.6",
    "vcl-core-modules": "~0.1.3",
    "vcl-default-theme": "~0.1.3",
    "vcl-default-theme-terms": "~0.1.0",
    "vcl-font-awesome": "~0.1.0",
    "vcl-preprocessor": "~0.1.1",
    "webpack": "^2.3.0",
    "webpack-dev-server": "^2.4.2",
    "zone.js": "^0.8.5"
  },
  "pre-commit": [
    "lint"
  ]
}
